{
	"name": "kevoree-registry-client",
	"version": "3.0.0-alpha",
	"description": "Client REST API to communicate with a Kevoree registry",
	"main": "dist/index",
	"scripts": {
		"start": "npm run webpack",
		"test": "npm run lint && npm run mocha && npm run karma",
		"mocha": "mocha test/spec",
		"karma": "karma start",
		"lint": "./node_modules/.bin/eslint src",
		"clean-dist": "node ./bin/clean-dist.js",
		"build": "npm run clean-dist && tsc && npm run webpack-build",
		"build-prod": "npm run clean-dist && NODE_ENV=production npm run webpack-build-prod",
		"webpack": "./node_modules/.bin/webpack-dev-server --progress --colors --hot --inline",
		"webpack-build": "./node_modules/.bin/webpack --progress",
		"webpack-build-prod": "./node_modules/.bin/webpack --progress -p",
		"prepublish": "npm run build-prod"
	},
	"keywords": [
		"kevoree",
		"registry",
		"client",
		"rest",
		"api"
	],
	"author": "Maxime Tricoire <max.tricoire@gmail.com> (https://github.com/maxleiko)",
	"repository": {
		"type": "git",
		"url": "https://github.com/kevoree/kevoree-js-registry-client"
	},
	"bugs": {
		"url": "https://github.com/kevoree/kevoree-js-registry-client/issues"
	},
	"license": "LGPL-3.0",
	"devDependencies": {
		"@types/chai": "^3.5.2",
		"@types/mocha": "^2.2.41",
		"@types/node": "^7.0.18",
		"@types/node-fetch": "^1.6.7",
		"babel-core": "^6.24.1",
		"babel-loader": "^7.0.0",
		"babel-preset-env": "^1.4.0",
		"chai": "^3.5.0",
		"debug": "^2.6.6",
		"eslint": "^3.19.0",
		"flow-bin": "^0.45.0",
		"karma": "^1.7.0",
		"karma-chai": "^0.1.0",
		"karma-chrome-launcher": "^2.1.1",
		"karma-firefox-launcher": "^1.0.1",
		"karma-mocha": "^1.3.0",
		"karma-webpack": "^2.0.3",
		"mocha": "^3.3.0",
		"rimraf": "^2.6.1",
		"tslib": "^1.7.0",
		"typescript": "^2.3.2",
		"webpack": "^2",
		"webpack-dev-server": "^2"
	},
	"dependencies": {
		"node-fetch": "^1.6.3",
		"tiny-conf": "^1.0.5"
	}
}
