{
  "name": "kevoree-registry-client",
  "version": "1.0.0-alpha",
  "description": "A library to communicate with a Kevoree registry REST API",
  "main": "dist/index",
  "scripts": {
    "test": "npm run build && npm run mocha && npm run karma",
    "mocha": "mocha build/test",
    "karma": "karma start",
    "clean": "node bin/clean.js",
    "tsc": "tsc -p src/main && tsc -p src/test",
    "build": "npm run clean && npm run tsc && npm run webpack",
    "build-prod": "npm run clean && npm run tsc && npm run webpack-prod",
    "webpack": "webpack --progress",
    "webpack-prod": "NODE_ENV=production webpack --progress -p"
  },
  "keywords": [
    "kevoree",
    "registry",
    "client",
    "rest",
    "api"
  ],
  "author": "Maxime Tricoire <max.tricoire@gmail.com> (https://github.com/maxleiko)",
  "repository": {
    "type": "git",
    "url": "https://github.com/kevoree/kevoree-js-registry-client"
  },
  "bugs": {
    "url": "https://github.com/kevoree/kevoree-js-registry-client/issues"
  },
  "license": "LGPL-3.0",
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.18",
    "@types/node-fetch": "^1.6.7",
    "@types/react": "^15.0.24",
    "@types/react-dom": "^15.5.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.4.0",
    "chai": "^3.5.0",
    "debug": "^2.6.6",
    "eslint": "^3.19.0",
    "karma": "^1.7.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-typescript": "^3.0.1",
    "karma-typescript-es6-transform": "^1.0.0",
    "mocha": "^3.3.0",
    "rimraf": "^2.6.1",
    "typescript": "^2.3.2",
    "webpack": "^2",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "node-fetch": "^1.6.3",
    "tiny-conf": "^1.0.5"
  }
}
